package Multas;

public class Main {
    public static void main(String[] args) {

        Motorista m1 = new Motorista("Jo√£o Silva", "123456");
        Veiculo v1 = new Veiculo("ABC-1234", "Gol", m1);

        Calcularinfracao calculadora = new CalculadoraInfracaoPadrao();
        AvaliadorCnh avaliador = new AvaliadorCnhPadrao();

        RegistroInfracaoService registro =
                new RegistroInfracaoService(calculadora, avaliador);

